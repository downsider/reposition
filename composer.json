{
  "name": "lexide/reposition",
  "description": "A modular database abstraction system",
  "license": "MIT",
  "homepage": "https://github.com/lexide/reposition",
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\Reposition\\": "src",
      "Lexide\\Reposition\\Test\\": "test"
    }
  },
  "suggest": {
    "downsider/puzzle-di": "Auto configured dependency injection for modules"
  },
  "extra": {
    "downsider-puzzle-di": {
      "lexide/syringe": {
        "path": "config/syringe.yml"
      }
    }
  }
}
